# GuardiaVision Gemini Environment Configuration
# Copy this file to .env and fill in your values

# Google API Key (Required)
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Alternative API key variable name
GEMINI_API_KEY=your_google_api_key_here

# Detection Configuration (Optional - overrides config.json)
GEMINI_MODEL_NAME=gemini-2.5-flash-preview-05-20
GEMINI_TEMPERATURE=0.0
MIN_CONFIDENCE=0.7
MAX_OBJECTS=20

# Image Processing Configuration (Optional)
MAX_IMAGE_SIZE=1024
ENHANCE_IMAGES=true

# Logging Configuration (Optional)
LOG_LEVEL=INFO
LOG_FILE=logs/guardiavision.log

# Performance Monitoring (Optional)
ENABLE_PERFORMANCE_MONITORING=true

# Output Configuration (Optional)
DEFAULT_OUTPUT_DIR=results
SAVE_RAW_RESULTS=false

# Development Settings (Optional)
DEBUG_MODE=false
VERBOSE_LOGGING=false
